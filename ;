const canvas = document.querySelector("canvas");
const c = canvas.getContext("2d");

canvas.width = 1024;
canvas.height = 1000;

c.fillStyle = "white";
c.fillRect(0, 0, canvas.width, canvas.height);
const image = new Image("./Map-1.png");
image.src = "./Map-1.png";
console.log(image);

image.onload = () => {
  c.drawImage(image, -2000, -2000);
};
